{
	"name": "algoritmhelper",
	"displayName": "AlgoritmHelper",
	"publisher": "Incognitomodes",
	"description": "Help with algorithms",
	"icon": "images/icon.png",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:algoritmhelper.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "algoritmhelper.helloWorld",
				"title": "Hello World"
			}
		],
		"snippets": [
			{
				"language": "typescript",
				"path": "snippets/sorting.json"
			},
			{
				"language": "typescript",
				"path": "snippets/searching.json"
			},
			{
				"language": "javascript",
				"path": "snippets/node.json"
			},
			{
				"language": "typescript",
				"path": "snippets/angular.json"
			},
			{
				"language": "typescript",
				"path": "snippets/algorithms.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
